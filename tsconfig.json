{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "target": "ES6",
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "components/sections/BenefitsSection.jsx", "components/sections/CTASection.jsx", "components/sections/Footer.jsx", "components/sections/HeroSection.jsx", "components/sections/ProblemSolutionSection.jsx", "components/sections/ProcessSection.jsx", "components/sections/ServicesSection.jsx", "components/sections/TestimonialsSection.jsx", "app/page.jsx", "app/layout.jsx", "components/Navbar.jsx", "components/theme-provider.jsx"],
  "exclude": ["node_modules"]
}
